{
  "name": "prototyper",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "format": "prettier --write --plugin-search-dir=. ./**/*.svelte"
  },
  "devDependencies": {
    "@fontsource/fira-mono": "^4.5.10",
    "@neoconfetti/svelte": "^1.0.0",
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/kit": "next",
    "autoprefixer": "^10.4.13",
    "postcss": "^8.4.19",
    "postcss-load-config": "^4.0.1",
    "postcss-nested": "^6.0.0",
    "postcss-preset-env": "^7.8.3",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.8.1",
    "svelte": "^3.53.1",
    "svelte-preprocess": "^4.10.7",
    "vite": "^3.2.4"
  },
  "postcss": {
    "plugins": {
      "postcss-preset-env": {
        "stage": 1
      },
      "postcss-nested": {},
      "autoprefixer": {}
    }
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "useTabs": false,
    "semi": false,
    "singleQuote": true,
    "plugins": [
      "./node_modules/prettier-plugin-svelte"
    ],
    "pluginSearchDirs": [
      "."
    ],
    "overrides": [
      {
        "files": "*.svelte",
        "options": {
          "parser": "svelte"
        }
      }
    ]
  },
  "type": "module"
}
